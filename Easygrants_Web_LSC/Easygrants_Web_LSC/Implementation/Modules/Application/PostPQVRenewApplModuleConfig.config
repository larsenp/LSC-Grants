							<Filters>
								<Argument PropertyKey='ApplicantCategoryID' Value='' Operator='NotEqual'/>
							</Filters>
						</Requirement>
						<Results>
							<Result
								ResultCode='3'
								Message='Applicant Category is required.'
								MaxCount='0'/>
						</Results>
					</ValidationItem>

				</ValidationSection>
				<ValidationSection Key='ProjectInformation' DisplayName='Project and Subgrant Information' PageKey='ProjectInformation'>
					<ValidationItem>
						<Requirement PropertyKey='LscCompRenewApplSubgrant'>
							<Filters>
								<Argument PropertyKey='HasCurrentLscSubgrants' Value='' Operator='NotEqual' />
							</Filters>
						</Requirement>
						<Results>
							<Result
								ResultCode='3'
								Message='You must indicate whether any portion of your current LSC grant is being subgranted.'
								MaxCount='0'/>
						</Results>
					</ValidationItem>
					<ValidationItem>
						<Requirement PropertyKey='LscCompRenewApplSubgrant'>
							<Filters>
								<Argument PropertyKey='HasCurrentLscSubgrants' Value='True' Operator='Boolean' />
								<Argument PropertyKey='CurrentLscSubgrantsCount' Value='0' >
									<RelatedProperty PropertyKey='LscCompRenewApplSubgrantTotals'/>
								</Argument>
							</Filters>
						</Requirement>
						<Results>
							<Result
								ResultCode='3'
								Message='You have indicated that a portion of your current LSC grant is being subgranted. You must provide the number of PAI and non-PAI subgrants.'
								MinCount='1'/>
						</Results>
					</ValidationItem>
					<ValidationItem>
						<Requirement PropertyKey='LscCompRenewApplSubgrant'>
							<Filters>
								<Argument PropertyKey='HasCurrentLscSubgrants' Value='False' Operator='Boolean' />
								<Argument PropertyKey='CurrentLscSubgrantsCount' Value='0' Operator='GreaterThan' >
									<RelatedProperty PropertyKey='LscCompRenewApplSubgrantTotals'/>
								</Argument>
							</Filters>
						</Requirement>
						<Results>
							<Result
								ResultCode='3'
								Message='You have indicated that no part of your current LSC grant is being subgranted. You have provided the number of PAI and/or non-PAI subgrants. Please correct.'
